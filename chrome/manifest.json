{
	"manifest_version": 2,
	"name": "omxplayer",
	"version": "1.0",
	"browser_action": {
		"default_icon": "logo.png",	
		"default_popup": "popup.html"
	},
	"icons": {
		"16": "logo.png",
		"32": "logo.png",
		"48": "logo.png",
		"128": "logo.png",
		"152": "logo.png"
	},
	"permissions": [/*"proxy",*/ "webRequest","<all_urls>"],

	"background":{
		"scripts": ["background.js"]
	}
}
